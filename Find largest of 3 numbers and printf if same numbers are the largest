#include<stdio.h>
int main()
{
	int a,b,c,l,m,n,o;
	printf("Enter first number:");
	scanf("%d",&a);
	printf("Enter second number:");
	scanf("%d",&b);
	printf("Enter third number:");
	scanf("%d",&c);
	if(a==b && b==c)
	{
		printf("All numbers are same");
		return 0;
	}
	l=a>b?(a>c?a:c):(b>c?b:c);
	if(a==b)
	{
		m=a;
	}
	else if(a==c)
	{
		n=a;
	}
	else if(c==b)
	{
		o=c;
	}
	if (m==l)
	{
		printf("Largets is %d\n",m);
		printf("First and second are same");	
	}
	else if (n==l)
	{
		printf("Largets is %d\n",m);
		printf("First and third are same");	
	}
	else if (o==l)
	{
		printf("Largets is %d\n",m);
		printf("Third and second are same");	
	}
	else
	printf("Largest number is %d",l);
	return 0;
}
